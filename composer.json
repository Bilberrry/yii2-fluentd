{
    "name": "bilberrry/yii2-fluentd",
    "description": "Logging with Fluentd for Yii2",
    "keywords": ["yii2", "fluentd", "log"],
    "homepage": "https://github.com/bilberrry/yii2-fluentd",
    "license": "MIT",
    "minimum-stability": "dev",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Alex Mukho",
            "email": "alex@bilberrry.com"
        }
    ],
    "config": {
        "process-timeout": 1800
    },
    "require": {
        "php": ">=5.4.0",
        "fluent/logger": "v1.0.0"
    },
    "autoload": {
        "psr-4": {
            "bilberrry\\log\\": "src"
        }
    }
}
